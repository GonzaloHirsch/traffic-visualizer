{
  "name": "traffic-visualizer",
  "version": "1.0.0",
  "description": "All-in-one visualizer for Cloud Run traffic on GCP. It automatically discovers Cloud Run services, ensuring that the configuration is minimal.",
  "private": false,
  "author": {
    "name": "Gonzalo Hirsch",
    "email": "hirschgonzalo@gmail.com",
    "url": "https://gonzalohirsch.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GonzaloHirsch/traffic-visualizer.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GonzaloHirsch/traffic-visualizer/issues"
  },
  "main": "backend/index.ts",
  "scripts": {
    "server": "ts-node backend/index.ts",
    "server:dev": "nodemon backend/index.ts",
    "lint": "npx eslint .",
    "toc": "npx markdown-toc README.md --no-firsth1",
    "build:prod": "npm run dashboard:build",
    "dashboard:dev": "(cd dashboard && vite)",
    "dashboard:build": "(cd dashboard && rm -rf ../dist && vite build --outDir ../dist)",
    "dashboard:preview": "vite preview",
    "visualizer:dev": "concurrently -c \"yellow,green\" \"npm:server:dev\" \"npm:dashboard:dev\"",
    "visualizer": "concurrently -c \"yellow,green\" \"npm:server\" \"npm:dashboard:preview\"",
    "headless": "npm run server",
    "npx-start": "npm run visualizer",
    "release": "npm publish",
    "release:dry": "npm publish --dry-run"
  },
  "release": {
    "branches": [
      "main"
    ],
    "tagFormat": "v${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/exec",
        {
          "verifyReleaseCmd": "echo \"NEXT_RELEASE_VERSION=${nextRelease.version}\" >> $GITHUB_ENV"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "bin": "./bin.js",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "12.0.0",
    "@semantic-release/release-notes-generator": "11.0.4",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/nconf": "0.10.6",
    "@types/node": "20.12.7",
    "@vitejs/plugin-vue": "5.0.4",
    "eslint": "8.45.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "markdown-toc": "1.2.0",
    "nodemon": "3.1.0",
    "prettier": "3.0.0",
    "semantic-release": "23.0.8",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "vite": "5.2.10"
  },
  "dependencies": {
    "@google-cloud/logging": "11.0.0",
    "axios": "1.6.8",
    "concurrently": "8.2.2",
    "cors": "2.8.5",
    "express": "4.19.2",
    "nconf": "0.12.1",
    "vue": "3.4.21",
    "vue-google-charts": "1.1.0",
    "vue-router": "4.3.2",
    "vue3-slider": "1.9.0"
  }
}